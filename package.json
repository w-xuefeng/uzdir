{
  "name": "uzdir",
  "version": "1.0.8",
  "scripts": {
    "uzdir": "bun run uzdir.ts",
    "clean": "rimraf test/to",
    "test": "bun clean && bun uzdir -i test/from -o test/to -p 123456 --passwordMap test/password.json --ignore ignore.zip --filter 'filter.txt,*/**/.DS_Store' --zipFormat '.zip,.rar,.7z' --log",
    "test:nosubdir": "bun run test --fullpath false",
    "lang": "bun run uzdir.ts lang",
    "mcp": "bun run uzdir.ts mcp"
  },
  "scriptDescriptions": {
    "mcp": "启动 MCP (Model Context Protocol) 服务，用于与支持 MCP 的客户端集成，提供文件解压能力"
  },
  "bin": {
    "uzdir": "uzdir.ts"
  },
  "author": {
    "name": "w-xuefeng",
    "email": "w-xuefeng@outlook.com"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^24.7.0",
    "@types/node-7z": "^2.1.10",
    "rimraf": "^6.0.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.1",
    "7zip-bin-full": "^25.1.2",
    "ansi-colors": "^4.1.3",
    "cli-progress": "^3.12.0",
    "commander": "^12.0.0",
    "glob": "^11.0.3",
    "node-7z": "^3.0.0"
  },
  "type": "module"
}